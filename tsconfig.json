{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./components/*"],
      "@hooks/*":["./hooks/*"],
      "@data/*": ["./data/*"],
      "@constants/*": ["./constants/*"],
      "@clients/*": ["./clients/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx",  "src/**/*.ts", "src/**/*.tsx","src/**/*.js", "packages/*/src/**/*",".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
